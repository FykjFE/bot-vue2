{
  "name": "bot-vue2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "cross-env NODE_ENV=dev  run-p  serve:dev  gulp",
    "serve:dev": "cross-env NODE_ENV=dev  vue-cli-service serve --open",
    "build": "cross-env NODE_ENV=prod gulp && cross-env NODE_ENV=prod  vue-cli-service build",
    "gulp": " cross-env NODE_ENV=dev  gulp",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "lint:fix": "vue-cli-service lint --fix",
    "styleguide": "vue-cli-service styleguidist",
    "styleguide:build": "vue-cli-service styleguidist:build"
  },
  "dependencies": {
    "@vue/composition-api": "^1.0.0-beta.14",
    "axios": "^0.20.0",
    "core-js": "^3.6.5",
    "element-ui": "^2.4.5",
    "npm-run-all": "^4.1.5",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0",
    "vuex-persistedstate": "^4.0.0-beta.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "fs-extra": "^9.0.1",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-load-plugins": "^2.0.5",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.0.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.19.1",
    "safe-require": "^1.0.4",
    "sass-loader": "^8.0.2",
    "vue-cli-plugin-element": "~1.0.1",
    "vue-cli-plugin-styleguidist": "~4.33.1",
    "vue-styleguidist": "^4.33.1",
    "vue-template-compiler": "^2.6.11"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{vue,js,jsx}": [
      "prettier --write",
      "eslint --config .eslintrc.js"
    ]
  }
}